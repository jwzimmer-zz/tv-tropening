<option value="/pmwiki/pmwiki.php/Quotes/InternetArchive?action=edit">Quotes</option>
<option value="/pmwiki/pmwiki.php/Recap/InternetArchive?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/InternetArchive?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/InternetArchive?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/InternetArchive?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/WMG/InternetArchive?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/InternetArchive?action=edit">YMMV</option>
</select>
</li>
</ul>
