<option value="/pmwiki/pmwiki.php/Quotes/ChroniclesofMagic?action=edit">Quotes</option>
<option value="/pmwiki/pmwiki.php/Recap/ChroniclesofMagic?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/ChroniclesofMagic?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/ChroniclesofMagic?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/ChroniclesofMagic?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/ChroniclesofMagic?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/ChroniclesofMagic?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/ChroniclesofMagic?action=edit">YMMV</option>
</select>
</li>
