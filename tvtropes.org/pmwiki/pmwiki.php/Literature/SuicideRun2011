<option value="/pmwiki/pmwiki.php/Recap/SuicideRun2011?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/SuicideRun2011?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/SuicideRun2011?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/SuicideRun2011?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/SuicideRun2011?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/SuicideRun2011?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/SuicideRun2011?action=edit">YMMV</option>
</select>
</li>
</ul>
