<option value="/pmwiki/pmwiki.php/Recap/RevancheCycle?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/RevancheCycle?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/RevancheCycle?action=edit">Synopsis</option>
