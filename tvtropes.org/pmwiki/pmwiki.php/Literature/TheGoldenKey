<option value="/pmwiki/pmwiki.php/Recap/TheGoldenKey?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/TheGoldenKey?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/TheGoldenKey?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/TheGoldenKey?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/TheGoldenKey?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/TheGoldenKey?action=edit">WMG</option>
