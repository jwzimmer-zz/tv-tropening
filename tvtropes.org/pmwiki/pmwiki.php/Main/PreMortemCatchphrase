<option value="/pmwiki/pmwiki.php/Quotes/PreMortemCatchphrase?action=edit">Quotes</option>
<option value="/pmwiki/pmwiki.php/Recap/PreMortemCatchphrase?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/PreMortemCatchphrase?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/PreMortemCatchphrase?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/PreMortemCatchphrase?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/PreMortemCatchphrase?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/PreMortemCatchphrase?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/PreMortemCatchphrase?action=edit">YMMV</option>
</select>
</li>
</ul>
