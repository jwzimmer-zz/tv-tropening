<option value="/pmwiki/pmwiki.php/Synopsis/SlowMotionPassBy?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/SlowMotionPassBy?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/SlowMotionPassBy?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/SlowMotionPassBy?action=edit">WMG</option>
