<option value="/pmwiki/pmwiki.php/Quotes/WalkieTalkieGagOver?action=edit">Quotes</option>
<option value="/pmwiki/pmwiki.php/Recap/WalkieTalkieGagOver?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/WalkieTalkieGagOver?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/WalkieTalkieGagOver?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/WalkieTalkieGagOver?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/WalkieTalkieGagOver?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/WalkieTalkieGagOver?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/WalkieTalkieGagOver?action=edit">YMMV</option>
</select>
</li>
</ul>
