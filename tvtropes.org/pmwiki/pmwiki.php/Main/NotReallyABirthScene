<option value="/pmwiki/pmwiki.php/Synopsis/NotReallyABirthScene?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/NotReallyABirthScene?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/NotReallyABirthScene?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/NotReallyABirthScene?action=edit">WMG</option>
