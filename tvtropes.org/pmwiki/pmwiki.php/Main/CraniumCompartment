<option value="/pmwiki/pmwiki.php/Recap/CraniumCompartment?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/CraniumCompartment?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/CraniumCompartment?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/CraniumCompartment?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/CraniumCompartment?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/CraniumCompartment?action=edit">WMG</option>
