<option value="/pmwiki/pmwiki.php/Timeline/SeahorseSteed?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/SeahorseSteed?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/SeahorseSteed?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/SeahorseSteed?action=edit">YMMV</option>
</select>
</li>
</ul>
