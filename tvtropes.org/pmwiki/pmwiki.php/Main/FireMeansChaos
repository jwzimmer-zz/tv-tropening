<option value="/pmwiki/pmwiki.php/Synopsis/FireMeansChaos?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/FireMeansChaos?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/FireMeansChaos?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/FireMeansChaos?action=edit">WMG</option>
