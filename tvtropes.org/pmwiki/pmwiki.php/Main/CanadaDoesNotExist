<option value="/pmwiki/pmwiki.php/Synopsis/CanadaDoesNotExist?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/CanadaDoesNotExist?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/CanadaDoesNotExist?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/CanadaDoesNotExist?action=edit">WMG</option>
