<option value="/pmwiki/pmwiki.php/Synopsis/SuperStoicShopkeeper?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/SuperStoicShopkeeper?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/SuperStoicShopkeeper?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/SuperStoicShopkeeper?action=edit">WMG</option>
