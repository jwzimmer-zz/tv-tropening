<option value="/pmwiki/pmwiki.php/Recap/SmithicalMarriage?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/SmithicalMarriage?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/SmithicalMarriage?action=edit">Synopsis</option>
