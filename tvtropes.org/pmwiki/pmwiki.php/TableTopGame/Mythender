<option value="/pmwiki/pmwiki.php/Quotes/Mythender?action=edit">Quotes</option>
<option value="/pmwiki/pmwiki.php/Recap/Mythender?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/Mythender?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/Mythender?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/Mythender?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/Mythender?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/Mythender?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/Mythender?action=edit">YMMV</option>
</select>
</li>
