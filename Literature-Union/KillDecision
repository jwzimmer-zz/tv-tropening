<option value="/pmwiki/pmwiki.php/Recap/KillDecision?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/KillDecision?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/KillDecision?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/KillDecision?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/KillDecision?action=edit">Trivia</option>
