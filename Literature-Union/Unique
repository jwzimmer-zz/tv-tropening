<option value="/pmwiki/pmwiki.php/Trivia/Unique?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/Unique?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/Unique?action=edit">YMMV</option>
</select>
</li>
</ul>
