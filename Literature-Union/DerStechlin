<option value="/pmwiki/pmwiki.php/Recap/DerStechlin?action=edit">Recap</option>
<option value="/pmwiki/pmwiki.php/ReferencedBy/DerStechlin?action=edit">ReferencedBy</option>
<option value="/pmwiki/pmwiki.php/Synopsis/DerStechlin?action=edit">Synopsis</option>
<option value="/pmwiki/pmwiki.php/Timeline/DerStechlin?action=edit">Timeline</option>
<option value="/pmwiki/pmwiki.php/Trivia/DerStechlin?action=edit">Trivia</option>
<option value="/pmwiki/pmwiki.php/WMG/DerStechlin?action=edit">WMG</option>
<option value="/pmwiki/pmwiki.php/YMMV/DerStechlin?action=edit">YMMV</option>
</select>
</li>
</ul>
